  Abstract                 = {This document defines an architecture for implementing$\backslash$nscalable service differentiation in the Internet. This architecture$\backslash$nachieves scalability by aggregating traffic classification state which$\backslash$nis conveyed by means of IP-layer packet marking using the DS field$\backslash$n[DSFIELD]. Packets are classified and marked to receive a particular$\backslash$nper-hop forwarding behavior on nodes along their path. Sophisticated$\backslash$nclassification, marking, policing, and shaping operations need only be$\backslash$nimplemented at network boundaries or hosts. Network resources are$\backslash$nallocated to traffic streams by service provisioning policies which$\backslash$ngovern how traffic is marked and conditioned upon entry to a$\backslash$ndifferentiated services-capable network, and how that traffic is$\backslash$nforwarded within that network. A wide variety of services can be$\backslash$nimplemented on top of these building blocks.},
  ISBN                     = {2475}
}

@Article{Bozzano01052011,
  Title                    = {Safety, Dependability and Performance Analysis of Extended AADL Models},
  Author                   = {Bozzano, Marco and Cimatti, Alessandro and Katoen, Joost-Pieter and Nguyen, Viet Yen and Noll, Thomas and Roveri, Marco},
  Journal                  = {The Computer Journal},
  Year                     = {2011},
  Number                   = {5},
  Pages                    = {754-775},
  Volume                   = {54},

  Abstract                 = {This paper presents a component-based modelling approach to
 system-software co-engineering of real-time embedded systems,
 in particular aerospace systems. Our method is centred around
 the standardized Architecture Analysis and Design Language
 (AADL) modelling framework. We formalize a significant subset
 of AADL, incorporating its recent Error Model Annex for
 modelling faults and repairs. The major distinguishing
 aspects of this component-based approach are the possibility
 to describe nominal hardware and software operations, hybrid
 (and timing) aspects, as well as probabilistic faults and
 their propagation and recovery. Moreover, it supports dynamic
 (i.e. on-the-fly) reconfiguration of components and
 inter-component connections. The operational semantics gives
 a precise interpretation of specifications by providing a
 mapping onto networks of event-data automata. These networks
 are then subject to different kinds of formal analysis such
 as model checking, safety and dependability analysis and
 performance evaluation. Mature tool support realizes these
 analyses. The activities reported in this paper are carried
 out in the context of the correctness, modelling, and
 performance of aerospace systems, project which is funded by
 the European Space Agency.},
  Doi                      = {10.1093/comjnl/bxq024},
  Eprint                   = {http://comjnl.oxfordjournals.org/content/54/5/754.full.pdf+html},
  Url                      = {http://comjnl.oxfordjournals.org/content/54/5/754.abstract}
}

@Article{Braden1994,
